{
  "private": true,
  "name": "gorgon",
  "version": "0.0.0",
  "description": "Experimenting with React + Redux + TypeScript + Tailwind CSS + Parcel",
  "scripts": {
    "build": "parcel build src/index.html",
    "format": "prettier --write '{.storybook,src}/**/*'",
    "lint": "yarn lint:scripts && yarn lint:styles",
    "lint:scripts": "eslint '{.storybook,src}/**/*.{js,ts,tsx}'",
    "lint:styles": "stylelint 'src/**/*.css'",
    "prebuild": "yarn types:check && yarn lint && yarn test",
    "start": "parcel src/index.html",
    "storybook": "start-storybook",
    "test": "jest --passWithNoTests",
    "test:coverage": "yarn test --coverage",
    "test:watch": "yarn test --watch",
    "types:check": "tsc"
  },
  "engines": {
    "node": "14.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benjaminkimball/gorgon.git"
  },
  "author": "Benjamin Kimball <hello@benjaminkimball.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/benjaminkimball/gorgon/issues"
  },
  "homepage": "https://github.com/benjaminkimball/gorgon#readme",
  "dependencies": {
    "@loadable/component": "^5.14.1",
    "@reduxjs/toolkit": "^1.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/react": "^6.1.11",
    "@types/jest": "^26.0.19",
    "@types/loadable__component": "^5.13.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^26.6.3",
    "parcel": "^1.12.4",
    "prettier": "^2.2.1",
    "storybook": "^6.1.11",
    "stylelint": "^13.8.0",
    "stylelint-config-recommended": "^3.0.0",
    "tailwindcss": "^2.0.2",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.3"
  }
}

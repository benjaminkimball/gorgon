{
  "private": true,
  "name": "gorgon",
  "version": "0.0.0",
  "description": "Experimenting with React + Redux + TypeScript + Tailwind CSS + Parcel",
  "scripts": {
    "build": "parcel build src/index.html",
    "format": "prettier --write '{.storybook,src}/**/*'",
    "lint": "yarn lint:scripts && yarn lint:styles",
    "lint:scripts": "eslint '{.storybook,src}/**/*.{js,ts,tsx}'",
    "lint:styles": "stylelint 'src/**/*.css'",
    "prebuild": "yarn type-check && yarn lint && yarn test",
    "start": "parcel src/index.html",
    "storybook": "start-storybook",
    "test": "jest --passWithNoTests",
    "test:coverage": "yarn test --coverage",
    "test:watch": "yarn test --watch",
    "type-check": "tsc"
  },
  "engines": {
    "node": "12.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benjaminkimball/gorgon.git"
  },
  "author": "Benjamin Kimball <hello@benjaminkimball.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/benjaminkimball/gorgon/issues"
  },
  "homepage": "https://github.com/benjaminkimball/gorgon#readme",
  "dependencies": {
    "@loadable/component": "^5.13.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "@types/jest": "^26.0.10",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "24.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.4.2",
    "parcel": "^1.12.4",
    "prettier": "^2.1.1",
    "storybook": "^6.0.21",
    "stylelint": "^13.7.0",
    "stylelint-config-recommended": "^3.0.0",
    "tailwindcss": "^1.7.6",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.0.2"
  }
}
